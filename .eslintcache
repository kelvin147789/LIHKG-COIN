   setAccount(accounts[0]);  \n\n     console.log(\"Account connected\");\n\n     const lihkgCoinData = await LIHKGCOIN.networks[networkID]\n\n     if (lihkgCoinData && !deployed)\n     {\n       const lihkgCoin = await new web3js.eth.Contract(LIHKGCOIN.abi,lihkgCoinData.address)\n       console.log(lihkgCoin)\n      setlihkgCoins(lihkgCoin)\n\n\n      let lihkgCoinAdd = lihkgCoin._address;\n      await setlihkgCoinAddress(lihkgCoinAdd);\n      console.log(lihkgCoin._address)\n\n      const lihkgCoinBalance = await lihkgCoin.methods.balanceOf(accounts[0]).call()\n      await setlihkgCoingBalances(lihkgCoinBalance/uint256)\n\n      setDeployed(true)\n      \n     }\n\n     \n\n\n     \n    \n\n     \n   }\n \n \n   \n\n   \n   return false;\n }\n \n const refreshPage = ()=> {\n   window.location.reload(false);\n }\n\n\n//  Contract Method\nconst claimLIHKGC = async()=> {\n\n  if (lihkgCoins)\n  {\n    lihkgCoins.methods.claimLIHKGC().send({\n      from: account\n     \n    })\n    console.log(lihkgCoinAddress)\n    \n  }\n  \n}\n\n\nconst agree5DemandNot1Less = async()=> {\n\n  if (lihkgCoins)\n  {\n    lihkgCoins.methods.agreeStatement().send({\n      from: account\n     \n    })\n    console.log('Agree 5 Demand Not 1 Less')\n\n  }\n  \n}\n \n\n\n\n  return (\n    <div className=\"App\">\n       <h1>LIHKG COIN</h1>\n      <div></div>\n       <img src={lihkgIcon} alt=\"Lihkg Icon\"/>\n\n\n       {/* Connect Metamsk wallet */}\n\n    <div className=\"metaMaskButton\">\n    <div>MetaMask Wallet Connect:</div>\n    \n    <MetaMaskButton className=\"spacingTop\" onClick={ethEnabled,refreshPage}>Connect with Wallet</MetaMaskButton>\n    <div>\n    {account}\n    </div>\n    <div class=\"contractAddress\">\n      Contract Address: \n      </div>\n      <div>\n      {lihkgCoinAddress}\n    </div>\n    <div class=\"balance\">\n       LIHKGCOIN Balances:\n    </div>\n\n    <div >\n      {lihkgCoinBalances}\n    </div>\n\n\n    <div>\n    <div class=\"buttons\">\n  <div class=\"container\"onClick={agree5DemandNot1Less} >\n      <i class=\"btn effect04\" data-sm-link-text=\"缺一不可\" ><span>五大訴求</span></i>\n  </div>\n</div>\n    </div>\n\n   \n\n    <div>\n    <div class=\"buttons\">\n  <div class=\"container\"onClick={claimLIHKGC} >\n      <i class=\"btn effect04\" data-sm-link-text=\"請先同意五大訴求\" ><span>Claim LIHKGC</span></i>\n  </div>\n</div>\n    </div>\n   \n    \n\n    {/* {account} */}\n    </div>\n    \n\n\n{/* Connect Metamsk wallet */}\n     \n    </div>\n  );\n}\n\nexport default App;\n","/Users/ok/Desktop/Projects/lihkg-coin/src/reportWebVitals.js",[],"/Users/ok/Desktop/Projects/lihkg-coin/src/index.js",[],{"ruleId":"24","severity":1,"message":"25","line":2,"column":38,"nodeType":"26","messageId":"27","endLine":2,"endColumn":44},{"ruleId":"24","severity":1,"message":"28","line":8,"column":25,"nodeType":"26","messageId":"27","endLine":8,"endColumn":29},{"ruleId":"24","severity":1,"message":"29","line":8,"column":31,"nodeType":"26","messageId":"27","endLine":8,"endColumn":34},{"ruleId":"24","severity":1,"message":"30","line":8,"column":36,"nodeType":"26","messageId":"27","endLine":8,"endColumn":46},{"ruleId":"24","severity":1,"message":"31","line":8,"column":47,"nodeType":"26","messageId":"27","endLine":8,"endColumn":53},{"ruleId":"24","severity":1,"message":"32","line":8,"column":54,"nodeType":"26","messageId":"27","endLine":8,"endColumn":60},{"ruleId":"24","severity":1,"message":"33","line":8,"column":61,"nodeType":"26","messageId":"27","endLine":8,"endColumn":66},{"ruleId":"24","severity":1,"message":"34","line":24,"column":19,"nodeType":"26","messageId":"27","endLine":24,"endColumn":29},{"ruleId":"35","severity":1,"message":"36","line":136,"column":63,"nodeType":"37","messageId":"38","endLine":136,"endColumn":64},"no-unused-vars","'useRef' is defined but never used.","Identifier","unusedVar","'Flex' is defined but never used.","'Box' is defined but never used.","'EthAddress' is defined but never used.","'Loader' is defined but n